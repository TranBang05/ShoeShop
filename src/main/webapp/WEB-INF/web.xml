<?xml version="1.0" encoding="UTF-8"?>
<web-app id="/Midnight" version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>Logincontroller</servlet-name>
        <servlet-class>Controller.Logincontroller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BlogController</servlet-name>
        <servlet-class>Controller.BlogController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BlogDetailController</servlet-name>
        <servlet-class>Controller.BlogDetailController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddPostBlogController</servlet-name>
        <servlet-class>Controller.AddPostBlogController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BlogDeleteController</servlet-name>
        <servlet-class>Controller.BlogDeleteController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddCommnetController</servlet-name>
        <servlet-class>Controller.AddCommnetController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getCommentServlet</servlet-name>
        <servlet-class>Controller.getCommentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddProductFeedback</servlet-name>
        <servlet-class>Controller.AddProductFeedback</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Logincontroller</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BlogController</servlet-name>
        <url-pattern>/blog</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BlogDetailController</servlet-name>
        <url-pattern>/blog-details</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddPostBlogController</servlet-name>
        <url-pattern>/add-post</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BlogDeleteController</servlet-name>
        <url-pattern>/delete-blog</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddCommnetController</servlet-name>
        <url-pattern>/add-comment</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getCommentServlet</servlet-name>
        <url-pattern>/get-comments</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddProductFeedback</servlet-name>
        <url-pattern>/add-product-feedback</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
      30
    </session-timeout>
    </session-config>
</web-app>
